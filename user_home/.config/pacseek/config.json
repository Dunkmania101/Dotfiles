	"ShowPkgbuildInternally": true,
	"ComputeRequiredBy": true,
	"GlyphStyle": "ASCII",
	"DisableNewsFeed": false,
	"FeedURLs": "https://archlinux.org/feeds/news/",
	"FeedMaxItems": 5,
	"SaveWindowLayout": false,
	"LeftProportion": 4,
	"Transparent": false,
	"PackageColumnWidth": 0,
	"EnableAutoSuggest": false,
	"SepDepsWithNewLine": false
