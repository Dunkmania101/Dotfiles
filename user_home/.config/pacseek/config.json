{
	"AurRpcUrl": "https://aurapi.moson.org/rpc",
	"AurTimeout": 5000,
	"AurSearchDelay": 500,
	"AurUseDifferentCommands": true,
	"AurInstallCommand": "aura -A",
	"AurUpgradeCommand": "aura -Ayu",
	"DisableAur": false,
	"MaxResults": 500,
	"PacmanDbPath": "/var/lib/pacman/",
	"PacmanConfigPath": "/etc/pacman.conf",
	"InstallCommand": "aura -S",
	"UninstallCommand": "aura -Rs",
	"SysUpgradeCommand": "aura -Syu",
	"SearchMode": "Contains",
